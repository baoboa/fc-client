<!DOCTYPE node PUBLIC
 "-//freedesktop//DTD D-Bus Object Introspection 1.0//EN"
 "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node name="/" xmlns:doc="http://www.freedesktop.org/dbus/1.0/doc.dtd">

  <interface name="org.gnome.FleetCommander.DConf">

    <method name="WriteProfile">
      <arg name="filename" direction="in" type="s"/>
    </method>

  </interface>

  <interface name="org.gnome.FleetCommander.Profiles">

    <method name="Add">
      <arg name="json_data" direction="in" type="s"/>
    </method>

    <method name="AddFile">
      <arg name="json_file" direction="in" type="s"/>
    </method>

    <method name="Get">
      <arg name="uid" direction="in" type="s"/>
      <arg name="json_data" direction="out" type="s"/>
    </method>

    <method name="List">
      <arg name="uids" direction="out" type="as"/>
    </method>

  </interface>

  <interface name="org.gnome.FleetCommander.Sessions">

    <method name="Add">
      <arg name="bus_address" direction="in" type="s"/>
    </method>

  </interface>

</node>
